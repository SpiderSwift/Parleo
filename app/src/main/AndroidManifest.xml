<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.leathersoft.parleo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/CustomAppTheme">
        <activity android:name=".activity.LoginActivity" />
        <activity android:name=".activity.RegistrationFinishActivity" />
        <activity android:name=".activity.RegistrationContinueActivity" />
        <activity android:name=".activity.RegistrationStartActivity" />
        <activity android:name=".activity.MainActivity">
        </activity>
        <activity android:name=".activity.ProfileActivity" />
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
        <activity android:name=".activity.TabsActivity" >

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity android:name=".activity.MessagesActivity"></activity>
    </application>

</manifest>